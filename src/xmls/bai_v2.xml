<mujoco model="bai feet only">
    <compiler angle="radian" meshdir="./meshes/" />

    <option iterations="3" ls_iterations="5" timestep="0.002" integrator="Euler">
        <flag eulerdamp="disable" />
    </option>
    

    <default>
        <default class="humanoid">

            <geom condim="1" contype="0" conaffinity="0" />
            <site size="0.01" rgba="1 0 0 1" group="4" />
            <joint axis="0 0 1" damping="1.5" />

            <position inheritrange="1.0" kp="2" />
            <default class="mad_mot">
                <joint actuatorfrcrange="-1.5 1.5" range="-1.7 1.7" armature="0.01"
                    frictionloss="0.05" damping="0.2" />
            </default>

            <default class="imu">
                <site type="box" size="0.1 0.1 0.1" rgba="1 0 0 0" />
            </default>

            <default class="visual">
                <geom type="mesh" density="0" group="2" material="metal" contype="0" conaffinity="0" />
            </default>

            <default class="collision">
                <default class="foot1">
                    <geom pos="0.076 -0.0025 0.0" type="sphere" size="0.02" contype="1"
                        conaffinity="1"/>
                </default>
            </default>

        </default>
    </default>

    <asset>
        <material name="metal" rgba=".9 .9 .9 1.0" />

        <mesh name="hip" file="hip.stl" scale="0.001 0.001 0.001" />
        <mesh name="hip2hip_l" file="hip2hip_l.stl" scale="0.001 0.001 0.001" />
        <mesh name="hip2hip_r" file="hip2hip_r.stl" scale="0.001 0.001 0.001" />
        <mesh name="leg_up_l" file="leg_up_r.stl" scale="0.001 0.001 0.001" />
        <mesh name="leg_up_r" file="leg_up_l.stl" scale="0.001 0.001 0.001" />
        <mesh name="leg_low_l" file="leg_low_r.stl" scale="0.001 0.001 0.001" />
        <mesh name="leg_low_r" file="leg_low_l.stl" scale="0.001 0.001 0.001" />
    </asset>

    <worldbody>

        <body name="torso" pos="0 0 0.32">


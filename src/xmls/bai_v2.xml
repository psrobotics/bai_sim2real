            <site name="root" class="imu" />

            <camera name="front" pos="1.210 -0.435 0.32"
                xyaxes="0.342 0.940 0.000 -0.321 0.117 0.940" mode="trackcom" />
            <camera name="track" pos="0.805 -1.105 0.2"
                xyaxes="0.814 0.580 0.000 -0.140 0.196 0.971" mode="trackcom" />

            <geom name="hip_geom" mesh="hip" class="visual" />

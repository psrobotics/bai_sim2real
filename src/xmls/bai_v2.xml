                        <geom name="r_toe_contact" class="foot1" />
                        <site name="r_toe" class="imu" />
                    </body>

                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <position class="mad_mot" name="HIP_L" joint="HIP_L" />
        <position class="mad_mot" name="AB_L" joint="AB_L" />
        <position class="mad_mot" name="KNEE_L" joint="KNEE_L" />

        <position class="mad_mot" name="HIP_R" joint="HIP_R" />
        <position class="mad_mot" name="AB_R" joint="AB_R" />
        <position class="mad_mot" name="KNEE_R" joint="KNEE_R" />
    </actuator>

    <sensor>
        <!-- added sensors for mujoco playground-->
        <gyro site="root" name="gyro" />
        <velocimeter site="root" name="local_linvel" />
        <accelerometer site="root" name="accelerometer" />
        <framezaxis objtype="site" objname="root" name="upvector" />
        <framexaxis objtype="site" objname="root" name="forwardvector" />
        <framelinvel objtype="site" objname="root" name="global_linvel" />
        <frameangvel objtype="site" objname="root" name="global_angvel" />
        <framepos objtype="site" objname="root" name="position" />
        <framequat objtype="site" objname="root" name="orientation" />

        <framelinvel objtype="site" objname="l_toe" name="l_toe_global_linvel" />
        <framelinvel objtype="site" objname="r_toe" name="r_toe_global_linvel" />
        <framexaxis objtype="site" objname="l_toe" name="l_toe_upvector" />
        <framexaxis objtype="site" objname="r_toe" name="r_toe_upvector" />

        <framepos objtype="site" objname="l_toe" name="l_toe_pos" />
        <framepos objtype="site" objname="r_toe" name="r_toe_pos" />

        <force name="left_foot_force" site="l_toe" />
        <force name="right_foot_force" site="r_toe" />
    </sensor>

</mujoco>